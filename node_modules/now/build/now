 * excludes invalid rules for .*ignore files
 *
 * @param {String} path
 * @return {Function} filter fn
 */

var invalidFilter = function invalidFilter(path) {
  return !(
  /* commments */
  '#' === path[0] ||

  /* empty lines or newlines */
  !path.trim().length);
};

